  "files": [
    "lib/"
  ],
  "bugs": {
    "url": "https://github.com/rodrigocmoreira/sgt-fields/issues"
  },
  "homepage": "https://github.com/rodrigocmoreira/sgt-fields#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "istanbul": "^0.4.5",
    "mocha": "^10.2.0",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
